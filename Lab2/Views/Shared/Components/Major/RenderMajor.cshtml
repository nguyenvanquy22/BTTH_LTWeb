@model IEnumerable<Major>
<ul class="nav">
    @foreach(var m in Model)
    {
        <li class="nav-item" id="@m.MajorID">
            <a class="nav-link">@m.MajorName</a>
        </li>
    }
    <li class="nav-item" id="">
        <a class="nav-link">All</a>
    </li>
</ul>